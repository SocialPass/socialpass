{
  "version": "0.1.0",
  "license": "MIT",
  "name": "@socialpass/tokengate",
  "author": "rizzo",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "export NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006 --no-open",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/tokengate.esm.js",
  "size-limit": [
    {
      "path": "dist/tokengate.cjs.production.min.js",
      "limit": "1s"
    },
    {
      "path": "dist/tokengate.esm.js",
      "limit": "1s",
      "import": "{ TokenGate }"
    }
  ],
  "dependencies": {
    "ethers": "^5.5.1",
    "history": "^5.3.0",
    "react-router-dom": "^6.3.0",
    "wagmi": "0.2.24"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@size-limit/preset-big-lib": "^7.0.8",
    "@size-limit/webpack": "^7.0.8",
    "@size-limit/webpack-why": "^7.0.8",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addons": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "husky": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  }
}
